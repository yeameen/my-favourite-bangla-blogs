  [4;35;1mPost Create (0.000338)[0m   [0mINSERT INTO `posts` (`blog_post_id`, `updated_at`, `num_comments`, `rating_total`, `title`, `url`, `rating_average`, `rating_negative`, `rating_positive`, `blog_id`, `content`, `posted_at`, `created_at`) VALUES(NULL, '2008-05-09 12:26:18', NULL, NULL, 'একটা ভীষন নিষিদ্ধ কবিতা ( সুশীলরা পড়বেন না )', 'http://www.somewhereinblog.net/blog/valobashablog/28782666', NULL, NULL, NULL, 9, '\"পেঁয়াজে মেখে দু\'মুঠো স্বপ্ন চটকে খেলুম \"- <br />\r\nশুনে নড়ে চড়ে বসেন কেতাদুরস্ত মন বিশেষজ্ঞ । <br />\r\nদাঁতের ফাঁকে অরিজিনাল পার্কারের খাপ গুঁজে <br />\r\nরায় দিয়ে দেন \" স্বপ্ন চটকে --- ইন্টারেস্টিং\" । <br />\r\nআমার সবটাই তাই ! <br />\r\n<br />\r\n\"তা\'পর স্বপ্নীল ভাবালুতার ধোঁয়া গিলে <br />\r\nফার্মগেট , শ\'বাগ ,টিসসি , বুঝলেন? <br />\r\nস্বপ্ন লটকে দিলুম অশ্বথের মাথায় ।\" <br />\r\nউত্তর কি আসবে জানেন তো ? ইন্টারেস্টিং ! <br />\r\n<br />\r\n\"বাইত ফিরে লিকলিকে গা থেকে স্বপ্ন খুলে <br />\r\nপাক জলে নাক ডুবিয়ে ঘুমানো ,আহ! \" <br />\r\n( আমার জিবে বিড়ির স্বাদে সুখ ভাসে <br />\r\nবাংলা খাব , বাংলা খাব ) <br />\r\nআবার পিছন থেকে ঘাই মারে -ইন্টারেস্টিং ! <br />\r\n<br />\r\nবাংলার স্বাদে এইবার আমার জিব টকায় । <br />\r\n\"তা\'পর কোমর থেকে স্বপ্ন খুলে স্বপ্নকেই ডাকা <br />\r\nআর দাঁড়িয়ে পড়া স্বপ্ন দেখা ,হো হো হো ।\" <br />\r\nশীত ঘরে এইবার কেতার কপালে ঘাম <br />\r\nপিছন থেকে উঠে আশা \"সব ইয়ে\" তামাম ! <br />\r\n<br />\r\nকিন্তু আমারও মাল উঠেছে মাথায় বুঝলেন? <br />\r\n\"তা\'পর স্বপ্নের হাঁটু দু\'দিকে ফাঁক করে -- <br />\r\n\"মিস ফারিয়া আআআ \" আর্তনাদে দামী ঘর <br />\r\nআসবাব উলটে শৃঙ্খলা আসে । <br />\r\nমাত্র ১৫০০ টাকা বেতনের ইউনিফর্মের ধাক্কায় <br />\r\nরাস্তার ছেলে ফের রাস্তায় । <br />\r\n<br />\r\nওরা সুশীল তো ! শিক্ষিত ভদ্দরনোক । <br />\r\nওরা জানে না , চালের কেজি ৫০ হলে , <br />\r\nপরনের কাপড় দুর্লভ হলে , <br />\r\nমাথা গোঁজার ঠাঁই না হলে , <br />\r\n\"চাকুরী হয়েছে\" চিঠি না পেলে ,<br />\r\nপাত্রের হাটে চকচকে না হলে, <br />\r\nপাড়ার অবৈধ বাজারটা হঠাৎ উঠে গেলে , <br />\r\n<br />\r\nআমরা , <br />\r\nএই শহরের রাস্তার ছেলেরা <br />\r\nস্বাধীনতার সাঁইত্রিশ বছর পরে <br />\r\nসামরিক সবুজ শান্তির কালে <br />\r\nকেবল স্বপ্ন চুদি , স্বপ্ন চোদাই । <br />\r\n<br />\r\n', '2008-03-26 14:18:16', '2008-05-09 12:26:18')[0m

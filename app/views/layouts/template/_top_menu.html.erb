<% if active_user().nil? %>
  <%= link_to_unless_current("Login", login_url) %>
<% else %>
  <%= link_to_unless_current("Blogs", :controller => "blog", :action => "list") %>
  <%= link_to_unless_current("Posts", :controller => "blog", :action => "posts") %>
  <%= link_to_unless_current("Logout", logout_url) %>
<% end %>